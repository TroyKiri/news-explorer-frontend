{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": "off",
    "linebreak-style": 0,
    "no-useless-escape": 0,
    "eqeqeq": "off",
    "no-unused-vars": "off",
    "no-console":"off",
    "func-names":"off",
    "import/prefer-default-export": "off",
    "class-methods-use-this":"off",
    "no-param-reassign":"off",
    "prefer-arrow-callback":"off",
    "arrow-body-style":"off",
    "no-alert":"off",
    "no-restricted-globals":"off",
    "no-plusplus": "off",
    "no-shadow":"off"
  },
  "env": {
    "browser": true
  }
}